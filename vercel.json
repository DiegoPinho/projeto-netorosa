{
  "version": 2,
  "buildCommand": "python vercel_build.py",
  "outputDirectory": "staticfiles",
  "functions": {
    "api/index.py": {
      "maxDuration": 60,
      "runtime": "python3.12"
    }
  },
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/api/index.py"
    }
  ],
  "env": {
    "PYTHONUNBUFFERED": "1"
  }
}